<%= form_for :user, url: '/login' do |f| %>
    <div class='field'>
        <%= f.label :email %>
        <%= f.email_field :email %><br>
    </div>

    <div class='field'>
        <%= f.label :password %>
        <%= f.password_field :password %><br>
    </div>
    
    <%= f.submit "Log In" %>
  <% end %>